<h1>Tests</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Questions count</th>
    <th>Actions</th>
  </tr>
    <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= test.questions.count %></td>
      <td><%= link_to 'View', test_path(test) %></td>
    </tr>
  <% end %>
</table>
